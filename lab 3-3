#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main()
{
	int i,j;
	float tab[100],war=0,sr=0,br;
	float min, max;
	srand(time(NULL));
	for (i=0; i<100; i++)
	{
		tab[i]=(1.0*rand()/RAND_MAX);
		printf("Tab[%d]=%f\n",i,tab[i]);
	}
	min=tab[0];
	max=tab[0];
	for (i=1; i<100; i++)
	{
		if (min>tab[i])
		{
			min=tab[i];
		}
		if (max<tab[i])
		{
			max=tab[i];
		}
	}
	printf("Najmniejsza liczba = %f\n",min);
	printf("NajwiÄ™ksza liczba = %f\n",max);
	return 0;
}
